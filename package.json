    "iframe-resizer": "4.2.3",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "jest-puppeteer": "4.3.0",
    "js-yaml": "3.13.1",
    "jsdom": "15.2.0",
    "json-loader": "0.5.7",
    "json2csv": "4.5.4",
    "measure-text-width": "0.0.4",
    "millify": "3.1.2",
