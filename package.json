    "cssnano": "4.1.10",
    "current-device": "0.10.1",
    "date-fns": "2.15.0",
    "debug": "4.1.1",
    "ejs": "3.1.3",
    "ejs-loader": "0.5.0",
    "eslint": "7.6.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-react": "7.20.5",
    "eslint-watch": "7.0.0",
