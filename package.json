    "minimatch": "3.0.4",
    "node-sass": "4.13.0",
    "postcss-loader": "3.0.0",
    "prop-types": "15.7.2",
    "puppeteer": "2.0.0",
    "query-string": "6.8.3",
    "raf": "3.4.1",
    "react": "16.11.0",
    "react-chartjs-2": "2.8.0",
    "react-dom": "16.11.0",
    "react-ga": "2.7.0",
