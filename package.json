    "prompt": "1.0.0",
    "prop-types": "15.7.2",
    "puppeteer": "1.13.0",
    "query-string": "6.3.0",
    "raf": "3.4.1",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-ga": "2.5.7",
    "react-hot-loader": "4.8.0",
