    "@babel/register": "7.6.2",
    "@material-ui/core": "4.5.2",
    "@material-ui/icons": "4.5.1",
    "@rooks/use-window-size": "3.4.0",
    "autoprefixer": "9.7.1",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-root-import": "6.4.1",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "babel-polyfill": "6.26.0",
    "babel-preset-latest-node": "3.2.1",
    "bluebird": "3.7.1",
    "browser-sync": "2.26.7",
    "caniuse-lite": "1.0.30001006",
    "chalk": "2.4.2",
    "change-case": "3.1.0",
    "chart.js": "2.9.1",
    "classnames": "2.2.6",
    "colors": "1.4.0",
    "connect": "3.7.0",
    "connect-history-api-fallback": "1.6.0",
    "connected-react-router": "6.5.2",
    "css-loader": "3.2.0",
    "cssnano": "4.1.10",
    "current-device": "0.9.1",
    "debug": "4.1.1",
    "ejs": "2.7.1",
    "ejs-loader": "0.3.5",
    "eslint": "6.6.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-react": "7.16.0",
    "eslint-watch": "6.0.1",
    "event-emitter": "0.3.5",
    "feed": "4.0.0",
    "file-loader": "4.2.0",
    "format-number": "3.0.0",
    "git-branch": "2.0.1",
