    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "babel-polyfill": "6.26.0",
    "babel-preset-latest-node": "3.1.1",
    "bluebird": "3.5.3",
    "browser-sync": "2.26.3",
    "chalk": "2.4.2",
    "change-case": "3.1.0",
    "cheerio": "1.0.0-rc.2",
    "classnames": "2.2.6",
    "colors": "1.3.3",
    "comma-number": "2.0.0",
    "connect": "3.6.6",
    "connect-history-api-fallback": "1.6.0",
    "connected-react-router": "6.3.2",
    "css-loader": "2.1.1",
    "cssnano": "4.1.10",
    "current-device": "0.8.0",
    "debug": "4.1.1",
    "ejs": "2.6.1",
    "ejs-loader": "0.3.1",
    "eslint": "5.15.1",
