    "eslint-plugin-react": "7.12.4",
    "eslint-watch": "5.0.1",
    "event-emitter": "0.3.5",
    "feed": "2.0.4",
    "file-loader": "3.0.1",
    "format-number": "3.0.0",
    "git-branch": "2.0.1",
    "git-last-commit": "0.3.0",
